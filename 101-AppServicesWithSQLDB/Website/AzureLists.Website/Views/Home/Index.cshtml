@{
    ViewBag.Title = "All Lists";
}
@model AzureLists.Website.Models.ListsViewModel



<div id="wrapper" class="wrapper @(Request.Browser.IsMobileDevice == true ? "sidebar-small" : "" ) @(Model.SelectedTask == null ? "edit-task-collapsed":"")">
    @Html.Partial("~/Views/Home/_lists.cshtml", Model.Lists)
    @Html.Partial("~/Views/Home/_tasks.cshtml", Model.SelectedList)
    @Html.Partial("~/Views/Home/_task.cshtml", Model.SelectedTask)

</div>


        
